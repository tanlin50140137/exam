<script>
function cearter_key()
{
	location.href="<?php echo apth_url('index.php/getkey');?>";
}
function add_classify()
{
	var f = $(".input_lists2").val();
	if( f == '' )
	{
		layer.tips('选择文件','.input_lists2');
		return false;
	}	
	$("#ify_frms").submit();
}
function Diachronic(){
	window.history.go(-1);
}
