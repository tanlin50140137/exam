});
function getImgApth(p)
{
	layer.open({
	  type: 1,
	  title: false,
	  closeBtn: 0,
	  skin: 'layui-layer-nobg',
	  shadeClose: true,
	  content: '<img src="'+p+'"/>'
	});
}
function getVodieApth(p)
{
	var html = '<div style="border:1px solid #0e0e0e;width:780px;height:450px;margin:10px auto;" id="video"></div>';
	layer.open({
	  title:false,
	  type: 1,
	  skin: 'layui-layer-demo',
	  closeBtn: 0,
	  area: '800px',
	  anim: 2,
	  shadeClose: true,
	  content: html
	});
	var videoObject = {
			container: '#video',
			variable: 'player',
			flashplayer:false,
			video:p
	};
	var player=new ckplayer(videoObject);
}
function getOfficeApth(p)
{
	open(p,'new');
}
var index;
function setUpdateFile(p,n)
{
	index = layer.open({
		 type: 2,
		 title: 'TanLin '+n+' 可编辑文档',
		 shadeClose: true,
		 shade: 0.8,
		 area: ['1000px', '95%'],
		 content: p
	}); 	
}
function delete_files(p)
{
	layer.confirm('确定要删除文件？', {
		  btn: ['确定','取消'] //按钮
		}, function(){
		  layer.msg('正在删除中......', {icon: 1},function(){
			  $.post("<?php echo apth_url('index.php');?>",{'act':'delete_files','apth':p},function(d){
					if( d == 'OK' )
					{
						layer.msg('删除成功',{time:1000},function(){		
							location.reload();
						});
					}
				});
		  });		  
		}, function(){});
}
function Establish(p)
{
	var html = '<div style="width:780px;height:450px;margin:10px auto;" id="video">';
		html += '<div style="border:1px solid #f3ecec;height:50px;line-height:50px;text-align:left;padding-left:10px;">'+decodeURIComponent(p)+'</div>';
		html += '<div style="border:1px solid #f3ecec;height:380px;line-height:380px;text-align:left;padding-left:10px;margin-top:10px;text-align:center;"><a href="javascript:void(0);" style="font-size:22px;color:#666666;" onclick="CreateDirectory(1,\''+p+'\');" onmouseover="CreateDirectoryCss1(this);" onmouseout="CreateDirectoryCss2(this);"><img src="data:image/png;base64," align="absmiddle"> 创建目录+</a> &nbsp; &nbsp; &nbsp; &nbsp; ';
		html += '<a href="javascript:void(0);" style="font-size:22px;color:#666666;" onclick="CreateDirectory(2,\''+p+'\');" onmouseover="CreateDirectoryCss1(this);" onmouseout="CreateDirectoryCss2(this);"><img src="data:image/png;base64," align="absmiddle"> 创建文件+</a>';
		html += '</div></div>';
	layer.open({
		  title:false,
		  type: 1,
		  skin: 'layui-layer-demo',
		  closeBtn: 0,
		  anim: 2,
		  area: '800px',
		  shadeClose: true,
		  content: html
	});
}
var tishi;
function CreateDirectory(i,p)
{
	if( i == 1 )
	{
		tishi = layer.prompt({title: '<img src="data:image/png;base64," align="absmiddle"> 输入目录名', formType: 0}, function(pass, index){
			  layer.close(index);			  
			  $.post('<?php echo apth_url('index.php');?>',{'act':'CreateDirectory','f':i,'n':pass,'p':p},function(d){
				  if( d == 'OK' ){
					  layer.msg('目录 '+ pass +' 创建成功',{time:1000},function(){
						  location.reload();
					  });
				  }else{
					  layer.msg(pass+'目录创建失败');
				  }
			  });
		});
	}
	else if( i == 2 )
	{
		tishi = layer.prompt({title: '<img src="data:image/png;base64," align="absmiddle"> 输入文件名', formType: 0}, function(pass, index){
			  layer.close(index);		  
			  $.post('<?php echo apth_url('index.php');?>',{'act':'CreateDirectory','f':i,'n':pass,'p':p},function(d){
				  if( d == 'OK' ){
					  layer.msg('文件 '+ pass +' 创建成功',{time:1000},function(){
						  location.reload();
					  });
				  }else{
					  layer.msg(pass+'文件创建失败');
				  }
			  });
		});
	}	
}
function CreateDirectoryCss1(t)
{
	$(t).css({"color":"red","text-decoration":"underline"});
}
function CreateDirectoryCss2(t)
{
	$(t).css({"color":"#666666","text-decoration":"none"});
}
</script>
